		<class>com.ims.security.dal.bean.Rule</class>
		<class>com.ims.security.dal.bean.UserProfile</class>
		<class>com.ims.security.dal.bean.UserRoleInstituteBranchModule</class>
		<class>com.ims.security.dal.bean.UserRoleScreenElement</class>
		<class>com.ims.security.dal.bean.Users</class>
		
		
		 
		<properties>
			<property name="hibernate.hbm2ddl.auto" value="create" />
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQLDialect" />
			<property name="hibernate.show_sql" value="true" />
		</properties>
	</persistence-unit>

</persistence>